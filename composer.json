{
  "name": "pastuhov/yii2-yml-catalog",
  "description": "YML (Yandex Market Language) generator.",
  "keywords": [
    "YML",
    "yandex",
    "generator",
    "e-commerce"
  ],
  "homepage": "https://github.com/pastuhov/yii2-yml-catalog",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Kirill Pastukhov",
      "email": "kirill@pastukhov.su",
      "homepage": "https://github.com/pastuhov/",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=5.3.0",
    "yiisoft/yii2": ">=2.0.4",
    "pastuhov/php-exec-command": "dev-master",
    "pastuhov/php-file-stream": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "pastuhov\\ymlcatalog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pastuhov\\ymlcatalog\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
